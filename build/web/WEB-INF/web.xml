<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ServletMantDoctor</servlet-name>
        <servlet-class>servlets.ServletMantDoctor</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletLogIn</servlet-name>
        <servlet-class>servlets.ServletLogIn</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletMenu</servlet-name>
        <servlet-class>servlets.ServletMenu</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletMantPaciente</servlet-name>
        <servlet-class>servlets.ServletMantPaciente</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletMantHerramientaMedica</servlet-name>
        <servlet-class>servlets.ServletMantHerramientaMedica</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletMantDoctor</servlet-name>
        <url-pattern>/ServletMantDoctor</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletLogIn</servlet-name>
        <url-pattern>/ServletLogIn</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletMenu</servlet-name>
        <url-pattern>/ServletMenu</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletMantPaciente</servlet-name>
        <url-pattern>/ServletMantPaciente</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletMantHerramientaMedica</servlet-name>
        <url-pattern>/ServletMantHerramientaMedica</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
